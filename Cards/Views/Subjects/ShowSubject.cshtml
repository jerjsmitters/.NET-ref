@model Cards.ViewModels.ShowSubjectViewModel

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div> 
    <ul>
        @foreach (var set in Model.Subject.Sets)
        {
            <li><a href="@Url.Action("ShowSet", "Sets", new {SetId = set.SetId})">@set.Name</a></li>
        }
    </ul>
</div>

